
<h1>"Login"</h1>

<p>Don't have an account? <%= link_to "Sign up",new_user_path %></p>
<%= form_tag sessions_path do%>
<p>
	<%= label_tag :uname, "Username or Email" %><br>
	<%= text_field_tag :uname, params[:uname] %>
</p>

<p>
	<%= label_tag :password %><br>
	<%= password_field_tag :password %>
</p>
<p><%= submit_tag "login" %>

</p>
<% end %>